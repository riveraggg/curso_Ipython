   "source": [
    "#en una funcion se pueden poner argumentos opcionales\n",
    "#para esto se usa el igual\n",
    "def fuerza(v_r,t,gama=0,viento=[0,0],m=1,g=9.8):\n",
    "    #g = 9.8\n",
    "    #m=1.\n",
    "    #gama = 0.1\n",
    "    #v_r = [ry, rz, vy, vz]\n",
    "    dvz = -g/m - (gama/m)*v_r[3]+viento[1]\n",
    "    dvy = 0 - (gama/m)*v_r[2]+viento[0]\n",
    "    drz = v_r[3]\n",
    "    dry = v_r[2]\n",
    "    return [dry,drz,dvy,dvz]"
   ]
